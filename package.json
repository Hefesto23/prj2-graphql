{
  "name": "prj2-graphql",
  "version": "2.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "dev": "env-cmd -f ./config/dev.env nodemon src/main.js --ext js,graphql --exec babel-node",
